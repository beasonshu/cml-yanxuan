      <view class="{{ checkClass }}  cml-base cml-view cml-2b23c372">
        <image class="inner-icon  cml-base cml-image cml-2b23c372" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-2b23c372">{{label}}</text>
  </view>
</view>