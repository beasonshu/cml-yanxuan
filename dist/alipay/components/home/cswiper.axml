<view class="carousel  cml-base cml-view cml-6483f0af" onTouchMove="_cmlEventProxy" data-eventtouchmove="{{['forbinding']}}">
  <view class="carousel-container  cml-base cml-view cml-6483f0af"><swiper class="carousel-container  cml-base cml-carousel cml-6483f0af" indicator-dots="{{true}}" current="{{1}}" circular="{{true}}" autoplay="{{true}}">
    <swiper-item a:for="{{bannerImgList}}" a:for-index="i" a:for-item="item" data-idx="{{i}}" a:key="{{i}}" class=" cml-base cml-carousel-item cml-6483f0af">
      <image src="{{item.imgUrl}}" class="Img  cml-base cml-image cml-6483f0af" />
    </swiper-item>

  </swiper></view>
</view>