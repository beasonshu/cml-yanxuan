<view class="carousel  cml-base cml-view" onTap="_cmlEventProxy" data-eventtap="forbinding" onCmlPropsEventProxy="_cmlPropsEventProxy">
  <view class="carousel-container  cml-base cml-view"><swiper class="carousel-container  cml-base cml-carousel" indicator-dots="{{true}}" current="{{1}}" circular="{{true}}" autoplay="{{true}}">
    <swiper-item a:for="{{bannerImgList}}" a:for-index="i" a:for-item="item" data-idx="{{i}}" class=" cml-base cml-carousel-item">
      <image src="{{item.imgUrl}}" class="Img  cml-base cml-image" />
    </swiper-item>

  </swiper></view>
</view>